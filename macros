function UntitledMacro() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('B5:B10').activate();
  spreadsheet.getRange('B4').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);
};

function onOpen() {
  // Get the spreadsheet owner.
  var owner = SpreadsheetApp.getActiveSpreadsheet().getOwner();

  // Give the owner edit permission.
  SpreadsheetApp.getActiveSpreadsheet().addEditor(owner);
};

function UntitledMacro1() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('S3').activate();
  spreadsheet.getCurrentCell().setValue('New');
  spreadsheet.getRange('S3:S103').activate();
  spreadsheet.getRange('S3').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);
};

function List_action_required_by_OPS() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('5:356').activate();
  spreadsheet.getRange('AD5').activate();
  spreadsheet.getRange('S5').activate();
  spreadsheet.getCurrentCell().offset(342, -1).activate();
};

function NEW_LIST_ACTION_REQUIRED() {
  var spreadsheet = SpreadsheetApp.getActive();
  var sheet = spreadsheet.getActiveSheet();
  sheet.getRange(spreadsheet.getCurrentCell().getRow(), 1, 352, sheet.getMaxColumns()).activate();
  spreadsheet.getCurrentCell().offset(0, 29).activate();
  spreadsheet.getCurrentCell().offset(0, -11).activate();
};

