  spreadsheet.getSelection().getNextDataRange(SpreadsheetApp.Direction.DOWN).activate();
  currentCell.activateAsCurrentCell();
  currentCell = spreadsheet.getCurrentCell();
  spreadsheet.getSelection().getNextDataRange(SpreadsheetApp.Direction.DOWN).activate();
  currentCell.activateAsCurrentCell();
  var sheet = spreadsheet.getActiveSheet();
  spreadsheet.getRangeList([sheet.getRange(1, spreadsheet.getCurrentCell().getColumn(), sheet.getMaxRows(), 1).getA1Notation(),
  sheet.getRange(1, spreadsheet.getCurrentCell().getColumn() + 2, sheet.getMaxRows(), 1).getA1Notation(),
  sheet.getRange(1, spreadsheet.getCurrentCell().getColumn() + 4, sheet.getMaxRows(), 1).getA1Notation()]).activate()
  .setNumberFormat('dd/MM/yyyy');
  spreadsheet.getCurrentCell().offset(1, 0, 3, 1).activate();
  spreadsheet.getActiveRangeList().setNumberFormat('#,##0.00')
  .setNumberFormat('#,##0.0')
  .setNumberFormat('#,##0')
  .setNumberFormat('#,##0');
  spreadsheet.getCurrentCell().offset(3, -6).activate();
  spreadsheet.getCurrentCell().getNextDataCell(SpreadsheetApp.Direction.PREVIOUS).activate();
  var conditionalFormatRules = spreadsheet.getActiveSheet().getConditionalFormatRules();
  conditionalFormatRules.push(SpreadsheetApp.newConditionalFormatRule()
  .setRanges([spreadsheet.getActiveRange()])
  .whenCellNotEmpty()
  .setBackground('#B7E1CD')
  .build());
  spreadsheet.getActiveSheet().setConditionalFormatRules(conditionalFormatRules);
  spreadsheet.getCurrentCell().offset(1408, 0).activate();
  spreadsheet.getCurrentCell().getNextDataCell(SpreadsheetApp.Direction.UP).activate();
  spreadsheet.getCurrentCell().offset(1, 0).activate();
  spreadsheet.getCurrentCell().getNextDataCell(SpreadsheetApp.Direction.UP).activate();
  spreadsheet.getCurrentCell().getNextDataCell(SpreadsheetApp.Direction.UP).activate();
  spreadsheet.getCurrentCell().offset(5, 0).activate();
  conditionalFormatRules = spreadsheet.getActiveSheet().getConditionalFormatRules();
  conditionalFormatRules.splice(1, 1, SpreadsheetApp.newConditionalFormatRule()
  .setRanges([spreadsheet.getCurrentCell().offset(-5, 0, 6, 1)])
  .whenFormulaSatisfied('=search($M$1,$AX:$AX)')
  .setBackground('#B7E1CD')
  .build());
  spreadsheet.getActiveSheet().setConditionalFormatRules(conditionalFormatRules);
  conditionalFormatRules = spreadsheet.getActiveSheet().getConditionalFormatRules();
  conditionalFormatRules.splice(1, 1, SpreadsheetApp.newConditionalFormatRule()
  .setRanges([spreadsheet.getCurrentCell().offset(-5, 0, 1422, 1)])
  .whenFormulaSatisfied('=search($M$1,$AX:$AX)')
  .setBackground('#B7E1CD')
  .build());
  spreadsheet.getActiveSheet().setConditionalFormatRules(conditionalFormatRules);
};



function UntitledMacro2() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('R:R').activate();
  spreadsheet.getActiveRangeList().setNumberFormat('dd/MM/yyyy');
  spreadsheet.getRange('P:P').activate();
  spreadsheet.getActiveRangeList().setNumberFormat('dd/MM/yyyy');
  spreadsheet.getRange('N:N').activate();
  spreadsheet.getActiveRangeList().setNumberFormat('dd/MM/yyyy');
};
